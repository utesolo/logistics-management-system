<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace 绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.lynz.logisticsmanagementsystem.mapper.OrdersMapper">
    <select id="getOrderByLogId" resultType="com.lynz.logisticsmanagementsystem.pojo.Orders">
        select * from orders where logistics_id = #{id}
    </select>
</mapper>